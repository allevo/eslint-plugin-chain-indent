  	"eslint",
  	"chain"
  ],
  "author": "allevo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allevo/eslint-plugin-chain-indent/issues"
  },
  "homepage": "https://github.com/allevo/eslint-plugin-chain-indent",
  "devDependencies": {
    "coveralls": "^2.11.13",
    "eslint": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
